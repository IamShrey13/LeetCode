class Solution {
public:
    int maxSubArray(vector<int>& nums) {
        int curr_sum=0, max_sum=INT_MIN, i;
        for(int val : nums){
            curr_sum+=val;
            max_sum=max(curr_sum, max_sum);
            if(curr_sum<0)
            curr_sum=0;
        }

        return max_sum;
    }
};
