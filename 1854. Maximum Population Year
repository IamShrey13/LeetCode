int maximumPopulation(int** logs,int logsSize,int* logsColSize){
    //declaring variables
    int timeline[101]={0},i,birth,death,maxpopulation=0,maxyear=1950,currentpopulation=0;

    //code
    for(i=0;i<logsSize;i++){
        birth=logs[i][0];
        death=logs[i][1];
        timeline[birth-1950]++;
        timeline[death-1950]--;
    }
    for(i=0;i<101;i++){
        currentpopulation+=timeline[i];
        if(currentpopulation>maxpopulation){
            maxpopulation=currentpopulation;
            maxyear=1950+i;
        }
        else if(currentpopulation==maxpopulation && (1950+i)<maxyear){
            maxyear=1950+i;
        }
    }
    return maxyear;
}
